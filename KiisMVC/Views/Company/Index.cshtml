@model IEnumerable<KiisMVC.Models.T_CompanyMaster>

@{
    ViewBag.Title = "Index";
}

<h3>事業所登録台帳</h3>

<table class="table table-hover table-condensed table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.記号)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.社保記号)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.番号)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.事業所名)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.事業主)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.郵便番号)
